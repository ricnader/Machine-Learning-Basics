# Symmetric matrices are matrices that even after being transposed,
# maintain the same order of the elements

import numpy as np

print("- Defining symmetric matrix")
X_sym = np.array([[0,1,2],[1,7,8],[2,8,9]])
print(X_sym)
print("")

print("- Transposing matrix")
X_trans = X_sym.T
print("")

print("- Even after transposed, the elements are still in their places")
print(X_sym == X_trans)
print("")